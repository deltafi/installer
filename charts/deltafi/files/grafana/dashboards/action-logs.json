                }
              },
              "type": "special"
            }
          ],
          "min": -6,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "rgb(222, 15, 43)",
                "value": null
              },
              {
                "color": "yellow",
                "value": 1
              },
              {
                "color": "rgb(222, 15, 43)",
                "value": 2
              }
            ]
          },
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 3,
        "w": 12,
        "x": 12,
        "y": 0
