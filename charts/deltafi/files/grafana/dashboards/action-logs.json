          "calcs": [
            "sum"
          ],
          "fields": "",
          "values": false
        },
        "textMode": "auto"
      },
      "pluginVersion": "9.1.1",
      "targets": [
        {
          "datasource": {
            "type": "loki",
            "uid": "${datasource}"
          },
          "editorMode": "builder",
          "expr": "sum(count_over_time({type=\"ACTION\", action=~\"$action_name\"} |~ `(?i)$searchable_pattern` [$__interval]))",
          "hide": false,
          "queryType": "range",
          "refId": "A"
        }
      ],
      "title": "Total Count: of $searchable_pattern",
      "type": "stat"
    },
    {
      "datasource": {
        "type": "loki",
        "uid": "${datasource}"
      },
      "description": "Live logs is a like 'tail -f' in a real time",
      "gridPos": {
        "h": 18,
        "w": 24,
        "x": 0,
        "y": 3
      },
      "id": 2,
      "interval": "10s",
      "options": {
        "dedupStrategy": "exact",
        "enableLogDetails": true,
        "prettifyLogMessage": true,
        "showCommonLabels": false,
        "showLabels": true,
        "showTime": false,
        "sortOrder": "Descending",
        "wrapLogMessage": true
      },
      "targets": [
        {
          "datasource": {
            "type": "loki",
            "uid": "${datasource}"
          },
          "editorMode": "builder",
          "expr": "{app=~\".+\", type=~\"ACTION\", action=~\"$action_name\"} |~ `(?i)$searchable_pattern`",
          "hide": false,
          "queryType": "range",
          "refId": "A"
        }
      ],
      "title": "Live logs",
      "type": "logs"
    }
